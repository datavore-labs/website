    "babel-register": "^6.7.2",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "cors": "^2.8.2",
    "css-loader": "^0.23.1",
    "cssnano": "^3.8.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
